{
    "name": "crosbies-n8n",
    "version": "2.0.0",
    "private": true,
    "scripts": {
        "gcp": "git add --all && git commit -m 'latest changes' && git push",
        "lint": "oxlint --type-aware",
        "px": "pixi run"
    },
    "prettier": {
        "arrowParens": "avoid",
        "experimentalOperatorPosition": "start",
        "experimentalTernaries": true,
        "objectWrap": "collapse",
        "plugins": [
            "prettier-plugin-packagejson"
        ],
        "printWidth": 100,
        "proseWrap": "never",
        "semi": false,
        "singleQuote": true,
        "tabWidth": 4,
        "overrides": []
    },
    "dependencies": {
        "@n8n/task-runner": "^1.52.0",
        "n8n": "1.115.3"
    },
    "devDependencies": {
        "@types/node": "^24.6.2",
        "oxlint": "1.19.0",
        "oxlint-tsgolint": "^0.2.0",
        "prettier": "3.6.2",
        "prettier-plugin-packagejson": "2.5.19",
        "typescript": "5.9.3"
    },
    "trustedDependencies": [
        "@parcel/watcher",
        "@sentry-internal/node-native-stacktrace",
        "protobufjs"
    ]
}
